<!DOCTYPE html>

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon"
        href="data:image/svg+xml;base64,"
        type="image/x-icon">
    <style>
        body {
            display: flex;
            justify-content: center;
            align-items: center;
            height: 100vh;
            margin: 0;
            font-family: Arial, sans-serif;
            text-align: center;
        }

        p {
            font-size: 1.5rem;
        }
    </style>
</head>

<body>
    <div>
        <p>请等待 <span id="time1">5</span> 秒</p>
        <p>Please wait <span id="time2">5</span> seconds</p>
    </div>
    <script>
        function setCookie(name, value, days) {
            var expires = "";
            if (days) {
                var date = new Date();
                date.setTime(date.getTime() + (6000 * 60 * 24 * days));
                expires = "; expires=" + date.toUTCString();
            }
            document.cookie = name + "=" + (value || "") + expires + "; path=/";
        }

        function getCookie(cname) {
            var name = cname + "=";
            var ca = document.cookie.split(';');
            for (var i = 0; i < ca.length; i++) {
                var c = ca[i].trim();
                if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
            }
            return "";
        }

        var time = 5;
        var id = setInterval(showTime, 1000);
        var token = 'hBPfWHBoJehGTPDHCWxPrjhiHKBbANSg';
        var renew = true;

        function showTime() {

            if (time <= 0) {

                if (renew) {
                    setCookie("wafauthorization", `${token}`, 1);
                }
                location.reload();
            } else {
                time--;
            }
            document.getElementById("time1").innerText = time;
            document.getElementById("time2").innerText = time;
        }
    </script>
</body>

</html>